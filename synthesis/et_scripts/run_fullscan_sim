#!/bin/ksh
 
# Template script for running Cadence Incisive simulator (NCSIM) on Cadence Encounter Test generated test vectors.
 
#  Generated by Encounter(R) RTL Compiler v14.10-s005_1
 
export WORKDIR=./et_scripts
 
irun \
   +nospecify \
   +access+rwc \
   +ncstatus \   +nc64bit \
   +TESTFILE1=$WORKDIR/testresults/verilog/VER.FULLSCAN.counter_atpg.data.scan.ex1.ts1.verilog \
   +TESTFILE2=$WORKDIR/testresults/verilog/VER.FULLSCAN.counter_atpg.data.logic.ex1.ts2.verilog \
   +HEARTBEAT \
   +FAILSET \
   +nctimescale+1ns/1ps \
   +ncoverride_timescale \
   +ncseq_udp_delay+2ps \
   +libext+.v+.V+.z+.Z+.gz \
   +nclibdirname+$WORKDIR/Inca_libs_00_23_29 \
   -l $WORKDIR/ncverilog_FULLSCAN.log \
  -v ../lib/slow_vdd1v0_basiccells.v \
$WORKDIR/counter.et_netlist.v \
$WORKDIR/testresults/verilog/VER.FULLSCAN.counter_atpg.mainsim.v

